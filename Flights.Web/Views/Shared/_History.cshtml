@model FlightTicketsSystem.Web.Models.HistoryViewModel


<div class="form-group">
    <label asp-for="TicketId" class="control-label"></label>
    <select asp-for="TicketId" asp-items="Model.Tickets" class="form-control"></select>
    <span asp-validation-for="TicketId" class="text-danger"></span>
</div>


<div class="form-group">
    <label asp-for="Ticket.Flight.CompleteFlight" class="control-label"></label>
    <textarea asp-for="Ticket.Flight.CompleteFlight" class="form-control"> </textarea>
    <span asp-validation-for="Ticket.Flight.CompleteFlight" class="text-danger"></span>
</div>

