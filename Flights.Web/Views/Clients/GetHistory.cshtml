@model FlightTicketsSystem.Web.Models.HistoryViewModel



<form asp-action="AddHistory" enctype="multipart/form-data">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <input type="hidden" asp-for="TicketId" />
    <input type="hidden" asp-for="UserId" />
    <div class="row center-block">
        <partial name="_History" />
    </div>
    <div class="form-group">
        <label asp-for="TicketId" class="control-label"></label>
        <select asp-for="TicketId" asp-items="Model.Tickets" class="form-control"></select>
        <span asp-validation-for="TicketId" class="text-danger"></span>
    </div>
</form>